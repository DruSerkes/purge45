{
    "manifest_version": 2,
    "name": "purge45",
    "description": "A simple chrome extention to purge 45's name from your browser",
    "version": "1.0.0",
    "browser_action": {
        "default_icon": {
            "16": "favicon-16x16.png",
            "32": "favicon-32x32.png",
            "48": "android-icon-48x48.png",
            "96": "favicon-96x96.png",
            "128": "icon-128x128.jpg"
        }
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "index.js"
            ]
        }
    ]
}